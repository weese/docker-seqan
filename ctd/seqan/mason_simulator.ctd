			<ITEM name="illumina-mismatch-quality-mean-begin" value="40.0" type="double" description="Mean PHRED quality for mismatch bases of first base in Illumina sequencing." required="false" advanced="false" />
			<ITEM name="illumina-mismatch-quality-mean-end" value="30.0" type="double" description="Mean PHRED quality for mismatch bases of last base in Illumina sequencing." required="false" advanced="false" />
			<ITEM name="illumina-mismatch-quality-stddev-begin" value="3.0" type="double" description="Standard deviation of PHRED quality for mismatch bases of first base in Illumina sequencing." required="false" advanced="false" />
			<ITEM name="illumina-mismatch-quality-stddev-end" value="15.0" type="double" description="Standard deviation of PHRED quality for mismatch bases of last base in Illumina sequencing." required="false" advanced="false" />
