	<PARAMETERS version="1.6.2" xsi:noNamespaceSchemaLocation="http://open-ms.sourceforge.net/schemas/Param_1_6_2.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<NODE name="mason_simulator" description="Read Simulation">
			<ITEM name="version-check" value="1" type="bool" description="Turn this option off to disable version update notifications of the application. " restrictions="1,ON,TRUE,T,YES,0,OFF,FALSE,F,NO" required="false" advanced="false" />
			<ITEM name="quiet" value="false" type="string" description="Low verbosity." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="verbose" value="false" type="string" description="Higher verbosity." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="very-verbose" value="false" type="string" description="Highest verbosity." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="seed" value="0" type="integer" description="Seed to use for random number generator." required="false" advanced="false" />
			<ITEM name="meth-seed" value="0" type="integer" description="Seed to use for methylation level random number generator." required="false" advanced="false" />
			<ITEM name="seed-spacing" value="2048" type="integer" description="Offset for seeds to use when multi-threading." required="false" advanced="false" />
			<ITEM name="num-threads" value="1" type="integer" description="Number of threads to use." restrictions="1:" required="false" advanced="false" />
			<ITEM name="force-single-end" value="false" type="string" description="Force single-end simulation although --out-right file is given." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="chunk-size" value="65536" type="integer" description="Number of fragments to simulate in one batch." restrictions="65536:" required="false" advanced="false" />
			<ITEM name="num-fragments" value="" type="integer" description="Number of reads/pairs to simulate." restrictions="1:" required="true" advanced="false" />
			<ITEM name="meth-fasta-in" value="" type="input_file" description="FASTA file with methylation levels of the input file." supported_formats="*.fa,*.fasta" required="false" advanced="false" />
			<ITEM name="out" value="" type="output_file" description="Output of single-end/left end reads." supported_formats="*.fa,*.fasta,*.fq,*.fastq,*.fq.gz,*.fastq.gz" required="true" advanced="false" />
			<ITEM name="out-right" value="" type="output_file" description="Output of right reads.  Giving this options enables paired-end simulation." supported_formats="*.fa,*.fasta,*.fq,*.fastq,*.fq.gz,*.fastq.gz" required="false" advanced="false" />
			<ITEM name="out-alignment" value="" type="output_file" description="SAM/BAM file with alignments." supported_formats="*.sam,*.bam" required="false" advanced="false" />
			<ITEM name="input-reference" value="" type="input_file" description="Path to FASTA file to read the reference from." supported_formats="*.fa,*.fasta" required="true" advanced="false" />
			<ITEM name="input-vcf" value="" type="input_file" description="Path to the VCF file with variants to apply." supported_formats="*.vcf" required="false" advanced="false" />
			<ITEM name="methylation-levels" value="false" type="string" description="Enable methylation level simulation." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="meth-cg-mu" value="0.6" type="double" description="Median of beta distribution for methylation level of CpG loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="meth-cg-sigma" value="0.03" type="double" description="Standard deviation of beta distribution for methylation level of CpG loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="meth-chg-mu" value="0.08" type="double" description="Median of beta distribution for methylation level of CHG loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="meth-chg-sigma" value="0.008" type="double" description="Standard deviation of beta distribution for methylation level of CHG loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="meth-chh-mu" value="0.05" type="double" description="Median of beta distribution for methylation level of CHH loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="meth-chh-sigma" value="0.005" type="double" description="Standard deviation of beta distribution for methylation level of CHH loci." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="fragment-size-model" value="normal" type="string" description="The model to use for the fragment size simulation." restrictions="normal,uniform" required="false" advanced="false" />
			<ITEM name="fragment-min-size" value="100" type="integer" description="Smallest fragment size to use when using uniform fragment size simulation." restrictions="1:" required="false" advanced="false" />
			<ITEM name="fragment-max-size" value="400" type="integer" description="Largest fragment size to use when using uniform fragment size simulation." restrictions="1:" required="false" advanced="false" />
			<ITEM name="fragment-mean-size" value="300" type="integer" description="Mean fragment size for normally distributed fragment size simulation." restrictions="1:" required="false" advanced="false" />
			<ITEM name="fragment-size-std-dev" value="30" type="integer" description="Fragment size standard deviation when using normally distributed fragment size simulation." restrictions="1:" required="false" advanced="false" />
			<ITEM name="seq-technology" value="illumina" type="string" description="Set sequencing technology to simulate." restrictions="illumina,454,sanger" required="false" advanced="false" />
			<ITEM name="seq-mate-orientation" value="FR" type="string" description="Orientation for paired reads.  See section Read Orientation below." restrictions="FR,RF,FF,FF2" required="false" advanced="false" />
			<ITEM name="seq-strands" value="both" type="string" description="Strands to simulate from, only applicable to paired sequencing simulation." restrictions="forward,reverse,both" required="false" advanced="false" />
			<ITEM name="embed-read-info" value="false" type="string" description="Whether or not to embed read information." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="read-name-prefix" value="simulated." type="string" description="Read names will have this prefix." required="false" advanced="false" />
			<ITEM name="enable-bs-seq" value="false" type="string" description="Enable BS-seq simulation." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="bs-seq-protocol" value="directional" type="string" description="Protocol to use for BS-Seq simulation." restrictions="directional,undirectional" required="false" advanced="false" />
			<ITEM name="bs-seq-conversion-rate" value="0.99" type="double" description="Conversion rate for unmethylated Cs to become Ts." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="illumina-read-length" value="100" type="integer" description="Read length for Illumina simulation." restrictions="1:" required="false" advanced="false" />
			<ITEM name="illumina-error-profile-file" value="" type="input_file" description="Path to file with Illumina error profile.  The file must be a text file with floating point numbers separated by space, each giving a positional error rate." supported_formats="*.txt" required="false" advanced="false" />
			<ITEM name="illumina-prob-insert" value="0.00005" type="double" description="Insert per-base probability for insertion in Illumina sequencing." restrictions="0:1" required="false" advanced="false" />
			<ITEM name="illumina-prob-deletion" value="0.00005" type="double" description="Insert per-base probability for deletion in Illumina sequencing." restrictions="0:1" required="false" advanced="false" />
