		<NODE name="razers3" description="Faster, fully sensitive read mapping">
			<ITEM name="version-check" value="1" type="bool" description="Turn this option off to disable version update notifications of the application. " restrictions="1,ON,TRUE,T,YES,0,OFF,FALSE,F,NO" required="false" advanced="false" />
			<ITEM name="percent-identity" value="95" type="double" description="Percent identity threshold." restrictions="50:100" required="false" advanced="false" />
			<ITEM name="recognition-rate" value="100" type="double" description="Percent recognition rate." restrictions="80:100" required="false" advanced="false" />
			<ITEM name="no-gaps" value="false" type="string" description="Allow only mismatches, no indels. Default: allow both." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="forward" value="false" type="string" description="Map reads only to forward strands." restrictions="true,false" required="false" advanced="false" />
			<ITEM name="reverse" value="false" type="string" description="Map reads only to reverse strands." restrictions="true,false" required="false" advanced="false" />
