			<ITEM name="mismatch-file" value="" type="string" description="Write mismatch patterns to FILE." required="false" advanced="false" />
			<ITEM name="compact-mult" value="2.2" type="double" description="Multiply compaction threshold by this value after reaching and compacting." restrictions="0:" required="false" advanced="false" />
			<ITEM name="no-compact-frac" value="0.05" type="double" description="Don&apos;t compact if in this last fraction of genome." restrictions="0:1" required="false" advanced="false" />
